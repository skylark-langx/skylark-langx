{
  "name": "skylark-langx",
  "version": "0.9.5",
  "description": "A simple JavaScript language extension library, including class support, Evented class, Deferred class and some commonly used tool functions.",
  "main": "./index",
  "scripts": {
    "build": "slib build ."
  },
  "homepage": "www.skylarkjs.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/skylarkjs/skylark-langx.git"
  },
  "keywords": [
    "skylark",
    "skylarkjs",
    "javacript",
    "class",
    "Evented",
    "Deferred"
  ],
  "author": "Hudaokeji Co.,Ltd",
  "license": "MIT",
  "bugs": "https://github.com/skylarkjs/skylark-langx/issues",
  "files": [
    "dist/",
    "skins/",
    "index.js"
  ],
  "dependencies": {
    "skylark-langx-arrays": "https://github.com/skylark-langx/skylark-langx-arrays.git",
    "skylark-langx-aspect": "https://github.com/skylark-langx/skylark-langx-aspect.git",
    "skylark-langx-async": "https://github.com/skylark-langx/skylark-langx-async.git",
    "skylark-langx-binary": "https://github.com/skylark-langx/skylark-langx-binary.git",
    "skylark-langx-chars": "https://github.com/skylark-langx/skylark-langx-chars.git",
    "skylark-langx-constructs": "https://github.com/skylark-langx/skylark-langx-constructs.git",
    "skylark-langx-datetimes": "https://github.com/skylark-langx/skylark-langx-datetimes.git",
    "skylark-langx-events": "https://github.com/skylark-langx/skylark-langx-events.git",
    "skylark-langx-funcs": "https://github.com/skylark-langx/skylark-langx-funcs.git",
    "skylark-langx-globals": "https://github.com/skylark-langx/skylark-langx-globals.git",
    "skylark-langx-hoster": "https://github.com/skylark-langx/skylark-langx-hoster.git",
    "skylark-langx-klass": "https://github.com/skylark-langx/skylark-langx-klass.git",
    "skylark-langx-maths": "https://github.com/skylark-langx/skylark-langx-maths.git",
    "skylark-langx-ns": "https://github.com/skylark-langx/skylark-langx-ns.git",
    "skylark-langx-numerics": "https://github.com/skylark-langx/skylark-langx-numerics.git",
    "skylark-langx-objects": "https://github.com/skylark-langx/skylark-langx-objects.git",
    "skylark-langx-paths": "https://github.com/skylark-langx/skylark-langx-paths.git",
   "skylark-langx-strings": "https://github.com/skylark-langx/skylark-langx-strings.git",
    "skylark-langx-topic": "https://github.com/skylark-langx/skylark-langx-topic.git",
    "skylark-langx-types": "https://github.com/skylark-langx/skylark-langx-types.git",
    "skylark-langx-urls": "https://github.com/skylark-langx/skylark-langx-urls.git"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "exports": {
    ".": "./dist/uncompressed/skylark-langx/main.js",
    "./*": "./dist/uncompressed/skylark-langx/*.js"
  },
  "skylarkjs": {
    "directories": {
      "dependencies": "./node_modules"
    },
    "secondaries": {},
    "build": {
      "prepare": {
        "texttojs": [
          "html",
          "json",
          "css"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      },
      "bundles": {
        "all": true
      }
    }
  }
}
